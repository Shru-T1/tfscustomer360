{
  "name": "material-dashboard-react",
  "version": "1.10.0",
  "description": "Material Dashboard React. Coded by Creative Tim",
  "private": false,
  "main": "dist/index.js",
  "dependencies": {
    "@babel/core": "7.14.0",
    "@fortawesome/fontawesome-free": "^5.15.4",
    "@material-ui/core": "4.11.4",
    "@material-ui/icons": "4.11.2",
    "chartist": "0.10.1",
